# Ingress envs
letsencrypt_email="helloworld@gmail.com"
fuel_faucet_ingress_dns="faucet.example.com"
fuel_faucet_ingress_http_port="80"

# EKS 
TF_VAR_eks_cluster_name="test-cluster"
