# k8s envs
k8s_provider="eks"
k8s_namespace="fuel-core"

# Faucet envs
fuel_faucet_image_repository="ghcr.io/fuellabs/faucet"
fuel_faucet_image_tag="latest"
fuel_faucet_pod_replicas="1"
human_logging="false"
wallet_secret_key="random"
captcha_secret="fuelrocks"
